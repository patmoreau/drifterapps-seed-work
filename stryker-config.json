{
  "stryker-config": {
    "baseline": {
      "provider": "Dashboard",
      "enabled": false
    },
    "coverage-analysis": "all",
    "ignore-methods": [
      "ToString",
      "ConfigureAwait",
      "*Exception.ctor",
      "Console.Write*"
    ],
    "reporters": ["html", "progress", "dashboard"],
    "target-framework": "net8.0",
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": null
    }
  }
}
